/*Escrever um programa que escreva seu nome, sua data de nascimento e o n√∫mero do seu telefone.*/

#include <stdio.h>

int main () {

	char nome;
	float data, numero;

	printf ("Digite seu nome: \n");
	scanf("%c", &nome);
	printf ("Qual sua data de nascimento: \n");
	scanf ("%d", &data);
	printf ("Qual seu numero de telefone: \n");
	scanf("%d", &numero);

}
